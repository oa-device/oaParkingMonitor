# oaParkingMonitor Environment Variables
# Copy to .env and fill in your values. Never commit .env!
# Config files contain only safe defaults - ALL secrets come from here.

# AWS Integration
CENTRAL_API_ENABLED=false
CENTRAL_API_URL=https://your-api.execute-api.region.amazonaws.com
API_KEY=your-aws-api-key
SECRET_KEY=your-aws-secret-key

# Device Identifiers
CUSTOMER_ID=your-customer-id
SITE_ID=your-site-id
ZONE_ID=your-zone-id
CAMERA_ID=your-camera-id

# Optional Overrides
BATCH_SIZE=10
SUBMISSION_INTERVAL=30
PHYSICAL_CAMERA_ID=/dev/video0
MODEL_PATH=models/yolo11m.pt
SNAPSHOT_INTERVAL=5
TOTAL_SPACES=50